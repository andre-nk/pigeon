{
  "name": "pigeon",
  "version": "2.1.0",
  "bin": {
    "omahti-cms-web-starter": "bin/install.mjs"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "cf-export": "contentful space export --config contentful/export-config.json --management-token %MANAGEMENT_TOKEN% --space-id %SPACE_ID%",
    "cf-generate-types": "cf-content-types-generator contentful/export.json -X --out contentful/types"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.19.4",
    "@radix-ui/react-dialog": "^1.1.4",
    "@studio-freight/lenis": "^1.0.37",
    "@tailwindcss/typography": "^0.5.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "contentful": "^10.6.21",
    "geist": "^1.3.1",
    "lucide-react": "^0.468.0",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "cf-content-types-generator": "^2.15.0",
    "contentful-cli": "^3.1.43",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{js,css,md,ts,tsx}": "prettier --write",
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
